<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
 "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
 
 <mapper namespace="com.withstudy.study.dao.StudyDAO">
 	<select id="selectStudyList" resultType="com.withstudy.study.model.Study">
 		SELECT
 			`id`
			, `userId`
			, `hostUserId`
			, `title`
			, `personnel`
			, `deadline`
			, `location`
			, `content`
			, `createdAt`
			, `updatedAt`
 		FROM
 			`study`
 		ORDER BY `id` DESC
 	</select>
 	
 	<select id="selectStudyById" parameterType="int" resultType="com.withstudy.study.model.Study">
 		SELECT
 			`id`
			, `userId`
			, `hostUserId`
			, `title`
			, `personnel`
			, `deadline`
			, `location`
			, `content`
			, `createdAt`
			, `updatedAt`
 		FROM
 			`study`
 		WHERE
 			`id` = #{studyId}
 	</select>
 	
 	<select id="selectStudyByuserId" resultType="com.withstudy.study.model.Study">
 		SELECT
 			`id`
			, `userId`
			, `hostUserId`
			, `title`
			, `personnel`
			, `deadline`
			, `location`
			, `content`
			, `createdAt`
			, `updatedAt`
 		FROM
 			`study`
 		WHERE
 			`userId` = #{userId}
 	</select>
 	<insert id="insertStudy" parameterType="map">
 		INSERT INTO `study`
 		(
 			`userId`
			, `hostUserId`
			, `title`
			, `personnel`
			, `deadline`
			, `location`
			, `content`
			, `createdAt`
			, `updatedAt`
 		)
 		VALUES
 		(
 			#{userId}
 			, #{hostUserId}
 			, #{title}
 			, #{personnel}
 			, #{deadline}
 			, #{location}
 			, #{content}
 			, NOW()
 			, NOW()
 		)
 	</insert>
 </mapper>